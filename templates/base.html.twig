<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}SelfBranding SARL{% endblock %}</title>

        <meta content="width=device-width, initial-scale=1.0" name="viewport" />
        <link rel="shortcut icon" href="{{ absolute_url(asset('favico.ico')) }}">
        {% block meta %}
            <meta name="description" content="Marque d'essence purement africaine, SelfBranding est une entreprise de services portée sur l'innovation, l'entrepreneuriat et la jeunesse." />
            <link rel="canonical" href="{{ absolute_url(path('app_home')) }}" />
            <meta property="og:locale" content="fr_FR" />
            <meta property="og:type" content="Item" />
            <meta property="og:title" content="SelfBranding SARL" />
            <meta property="og:description" content="Marque d'essence purement africaine, SelfBranding est une entreprise de services portée sur l'innovation, l'entrepreneuriat et la jeunesse." />
            <meta property="og:url" content="{{ absolute_url(path('app_home')) }}" />
            <meta property="og:site_name" content="SelfBranding" />
            <meta property="article:section" content="Item" />
            <meta property="article:published_time" content="2021-06-20T15:25:59+00:00" />
            <meta property="og:image" content="{{ absolute_url(asset('/embleme.jpg')) }}" />
            <meta name="twitter:card" content="summary"/>
            <meta name="twitter:description" content="Marque d'essence purement africaine, SelfBranding est une entreprise de services portée sur l'innovation, l'entrepreneuriat et la jeunesse."/>
            <meta name="twitter:title" content="SelfBranding SARL"/>
            <meta name="twitter:domain" content="SelfBranding"/>
            <meta name="twitter:image:src" content="{{ absolute_url(asset('/embleme.jpg')) }}"/>
        {% endblock %}
        <!--[if lt IE 9]>
        <script src="{{ absolute_url(asset('/frontend/js/html5shiv.js'))}}"></script>
        <![endif]-->
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" type="text/css" >
            <link rel="stylesheet" href="{{ absolute_url(asset('/frontend/css/selfbranding.css')) }}">
        {% endblock %}
        <!-- Load Facebook SDK for JavaScript -->
        <script>
            window.fbAsyncInit = function() {
                FB.init({
                    xfbml            : true,
                    version          : 'v11.0'
                });
            };

            (function(d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = 'https://connect.facebook.net/fr_FR/sdk/xfbml.customerchat.js';
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));</script>
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=G-35KRDJV7XV"></script>
        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());

            gtag('config', 'G-35KRDJV7XV');
        </script>
    </head>
    <body class="de_light">
    <div id="fb-root"></div>
        <div id="wrapper">

            <div id="btn-exit" class="dark">
                <div class="line-1"></div>
                <div class="line-2"></div>
            </div>
            <div class="transition"></div>

            <header class="header-light transparent">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="d-flex justify-content-between">
                                <div class="align-self-center header-col-left">
                                    <div id="logo">
                                        <a href="{{ absolute_url(path('app_home')) }}">
                                            SELFBRANDING
                                        </a>
                                    </div>
                                </div>
                                <div class="align-self-center ml-auto header-col-mid">
                                    <ul id="mainmenu" class="scrollnav">
                                        <li><a href="{{ absolute_url(path('app_home')) }}"><i class="fa fa-home"></i></a></li>
                                        <li></li>
                                        <li><a href="#selfbranding-presentation">Qui sommes-nous?</a></li>
                                        <li></li>
                                        <li><a href="#selfbranding-services">Nos services</a></li>
                                        <li></li>
                                        <li><a href="#selfbranding-encyclopreneur">Encyclopreneur</a></li>
                                        <li></li>
                                        <li><a href="#selfbranding-blog">Blog</a></li>
                                        <li></li>
                                        <li><a href="#selfbranding-contact">Contact</a></li>
                                        <li></li>
                                    </ul>
                                </div>
                                <div class="align-self-center ml-auto header-col-right">
                                    <div class="social-icons s-border sm-hide">
                                        <a href="https://facebook.com/selfbrandingci" target="_blank"><i class="fa fa-facebook fa-lg"></i></a>
                                        <a href="https://twitter.com/selfbrandingci" target="_blank"><i class="fa fa-twitter fa-lg"></i></a>
                                        <a href="#"><i class="fa fa-linkedin fa-lg"></i></a>
                                        <a href="https://instagram.com/selfbrandingci" target="_blank"><i class="fa fa-instagram fa-lg"></i></a>
                                    </div>
                                    <span id="menu-btn"></span>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <div class="no-bottom no-top">
                <div id="top"></div>

                {% block body %}{% endblock %}
                <div class="fb-customerchat"
                     attribution="install_email"
                     attribution_version="biz_inbox"
                     page_id="101785814969487">
                </div>
            </div>
        </div>

        {% block javascripts %}
            <script src="{{ absolute_url(asset('/frontend/js/jquery.min.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/jpreLoader.min.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/bootstrap.min.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/wow.min.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/jquery.isotope.min.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/easing.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/owl.carousel.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/validation.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/jquery.magnific-popup.min.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/enquire.min.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/jquery.stellar.min.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/jquery.plugin.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/typed.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/typed-custom.js')) }}"></script>
            <script src="{{ absolute_url(asset('/frontend/js/designesia.js')) }}"></script>
        {% endblock %}
    </body>
</html>
