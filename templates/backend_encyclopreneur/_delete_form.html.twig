<form method="post" action="{{ path('backend_encyclopreneur_delete', {'id': encyclopreneur.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette biographie?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ encyclopreneur.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
